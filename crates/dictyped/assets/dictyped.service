[Unit]
Description=Dictype Daemon
After=audio.target

[Service]
Type=simple
ExecStart=/usr/bin/dictyped
Restart=on-failure
RestartSec=5

RuntimeDirectory=dictype
RuntimeDirectoryMode=0700

CapabilityBoundingSet=
AmbientCapabilities=
ProtectSystem=strict
ProtectHome=read-only

[Install]
WantedBy=graphical-session.target